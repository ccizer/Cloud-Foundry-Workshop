{
  "instance_min_count": 1,
  "instance_max_count": 5,
  "scaling_rules": [
    {
      "metric_type": "memoryused",
      "threshold": 80,
      "operator": ">=",
      "adjustment": "+1"
    },
    {
      "metric_type": "memoryused",
      "threshold": 30,
      "operator": "<=",
      "adjustment": "-1"
    }
  ]
}

